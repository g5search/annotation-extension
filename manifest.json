{
  "name": "G5 Annotations",
  "version": "1.0.0",
  "description": "A Chrome Extension for the G5 Annotation Service",
  "permissions": [
    "storage",
    "declarativeContent"
  ],
  "background": {
    "scripts": ["src/background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "src/popup.html"
  },
  "manifest_version": 2
}
